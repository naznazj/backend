{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/backend/(.*)", "dest": "/index.js" }, 
      { "src": "/(.*)", "dest": "/src/index.html" }
    ]
  }
  